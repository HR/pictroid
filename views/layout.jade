block variables
	- var bodyStyle = ""
	- var headerClasses = ""
	- var user = { }
include components/mixins
doctype html
html
	head
		block head
		title= title
		
		// Meta
		meta(charset='utf-8')
		meta(name='viewport', content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no')
		if secure
			meta(http-equiv='cache-control', content='no-cache')
			meta(http-equiv='expires', content='0')
			meta(http-equiv='pragma', content='no-cache')
		// Favicon
		link(rel="icon", type="image/png", href="/images/favicon/64.png")

		// Stylesheets
		link(rel='stylesheet', href='//fonts.googleapis.com/css?family=Open+Sans:400,300')
		link(rel='stylesheet', href='/bootstrap/bootstrap.min.css')
		link(rel='stylesheet', href='/stylesheets/style.css')
		link(rel='/images/favicon/apple-touch-icon', sizes='57x57', href='/images/favicon/apple-touch-icon-57x57.png')
		link(rel='apple-touch-icon', sizes='114x114', href='/images/favicon/apple-touch-icon-114x114.png')
		link(rel='apple-touch-icon', sizes='72x72', href='/images/favicon/apple-touch-icon-72x72.png')
		link(rel='apple-touch-icon', sizes='144x144', href='/images/favicon/apple-touch-icon-144x144.png')
		link(rel='apple-touch-icon', sizes='60x60', href='/images/favicon/apple-touch-icon-60x60.png')
		link(rel='apple-touch-icon', sizes='120x120', href='/images/favicon/apple-touch-icon-120x120.png')
		link(rel='apple-touch-icon', sizes='76x76', href='/images/favicon/apple-touch-icon-76x76.png')
		link(rel='apple-touch-icon', sizes='152x152', href='/images/favicon/apple-touch-icon-152x152.png')
		link(rel='icon', type='image/png', href='/images/favicon/favicon-196x196.png', sizes='196x196')
		link(rel='icon', type='image/png', href='/images/favicon/favicon-160x160.png', sizes='160x160')
		link(rel='icon', type='image/png', href='/images/favicon/favicon-96x96.png', sizes='96x96')
		link(rel='icon', type='image/png', href='/images/favicon/favicon-16x16.png', sizes='16x16')
		link(rel='icon', type='image/png', href='/images/favicon/favicon-32x32.png', sizes='32x32')
		meta(name='Pictroid', content='#eee')
		meta(name='Pictroid', content='/images/favicon/mstile-144x144.png')
	body
		div(id="wrapper", class=bodyClasses)
			include components/header
			block content
		include components/footer		
		
		// Bootstrap core JavaScript
		// Placed at the end of the document so the pages load faster
		script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js?n=1")
		script(src="/bootstrap/bootstrap.min.js?n=1")
		script(src="//www.parsecdn.com/js/parse-1.2.18.min.js?n=1")